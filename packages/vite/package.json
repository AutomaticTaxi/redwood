    "vite": "4.5.2",
    "yargs-parser": "21.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@types/react": "^18.2.55",
    "@types/yargs-parser": "21.0.3",
    "glob": "10.3.10",
    "typescript": "5.3.3",
    "vitest": "1.2.2"
  },
