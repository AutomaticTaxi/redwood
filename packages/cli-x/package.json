    "directory": "packages/cli-x"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn node ./build.mjs && yarn build:types",
