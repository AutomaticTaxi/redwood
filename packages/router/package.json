{
  "name": "@redwoodjs/router",
  "version": "7.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/router"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./dist/namedRoutes": {
      "types": "./dist/namedRoutes.d.ts",
      "import": "./dist/namedRoutes.js",
      "default": "./dist/cjs/namedRoutes.js"
    },
    "./dist/route-validators": {
      "types": "./dist/route-validators.d.ts",
      "import": "./dist/route-validators.js",
      "default": "./dist/cjs/route-validators.js"
    },
    "./dist/util": {
      "types": "./dist/util.d.ts",
      "import": "./dist/util.js",
      "default": "./dist/cjs/util.js"
    },
    "./dist/router": {
      "types": "./dist/router.d.ts",
      "import": "./dist/router.js",
      "default": "./dist/cjs/router.js"
    },
    "./dist/index": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./dist/dummyComponent": {
      "types": "./dist/dummyComponent.d.ts",
      "import": "./dist/dummyComponent.js",
      "default": "./dist/cjs/dummyComponent.js"
    },
    "./dist/server-router": {
      "types": "./dist/server-router.d.ts",
      "import": "./dist/server-router.js",
      "default": "./dist/cjs/server-router.js"
    },
    "./dist/location": {
      "types": "./dist/location.d.ts",
      "import": "./dist/location.js",
      "default": "./dist/cjs/location.js"
    },
    "./dist/analyzeRoutes": {
      "types": "./dist/analyzeRoutes.d.ts",
      "import": "./dist/analyzeRoutes.js",
      "default": "./dist/cjs/analyzeRoutes.js"
    },
    "./dist/splash-page": {
      "types": "./dist/splash-page.d.ts",
      "import": "./dist/splash-page.js",
      "default": "./dist/cjs/splash-page.js"
    },
    "./dist/Route": {
      "types": "./dist/Route.d.ts",
      "import": "./dist/Route.js",
      "default": "./dist/cjs/Route.js"
    },
    "./dist/Set": {
      "types": "./dist/Set.d.ts",
      "import": "./dist/Set.js",
      "default": "./dist/cjs/Set.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "skip-nav.css"
  ],
  "scripts": {
    "build": "tsx ./build.mts && yarn build:types",
    "build:pack": "yarn pack -o redwoodjs-router.tgz",
    "build:types": "tsc --build --verbose tsconfig.build.json",
    "build:watch": "nodemon --watch src --ext \"js,jsx,ts,tsx\" --ignore dist --exec \"yarn build\"",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "vitest run",
    "test:types": "tstyche",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@redwoodjs/auth": "workspace:*"
  },
  "devDependencies": {
    "@redwoodjs/framework-tools": "workspace:*",
    "@testing-library/jest-dom": "6.4.5",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "react": "19.0.0-beta-04b058868c-20240508",
    "react-dom": "19.0.0-beta-04b058868c-20240508",
    "tstyche": "1.1.0",
    "tsx": "4.10.3",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  },
  "peerDependencies": {
    "react": "19.0.0-beta-04b058868c-20240508",
    "react-dom": "19.0.0-beta-04b058868c-20240508"
